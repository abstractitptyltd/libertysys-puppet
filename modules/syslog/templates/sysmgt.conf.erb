<% require '/etc/puppet/include/puppetutils.rb' -%>
<%= include_header( "configuration file: syslog local files" ) -%>

*.info		/var/log/sysmgt/all

<% priv_files.each do |f| -%>
<%= f %>.info	/var/log/sysmgt/<%= f %>
<% end -%>

<% files.each do |f| -%>
<%= f %>.info	/var/log/sysmgt/<%= f %>
<% end -%>

